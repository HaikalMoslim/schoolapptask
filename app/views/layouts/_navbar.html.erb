<nav>
<% if user_signed_in? %>
  <ul class="justify-center flex m-10">
    <li><%= link_to "Students", students_path %></li>
    <%if current_user.admin? || current_user.teacher? %>
    <li><%= link_to "Teachers", teachers_path %></li>
    <li><%= link_to 'Manage Enrollments', enrollments_path %></li>
    <%end%>
    <li><%= link_to 'Receipt', receipts_path %></li>
    <li><%= link_to "Payment", payments_path %></li>
    <li><%= link_to "Fee", fees_path %></li>
    <div>
      <% if user_signed_in? %>
        <% if current_user.admin? || current_user.student? || current_user.teacher? %>
          <%= button_to "Log Out", destroy_user_session_path, method: :delete %>
        <% else %>
          <%= link_to "Profile", edit_user_registration_path %>
          <%= button_to "Log Out", destroy_user_session_path, method: :delete %>
        <% end %>
      <% else %>
        <%= link_to "Login", new_user_session_path %>
        <%= link_to "Sign Up", new_user_registration_path %>
      <% end %>
    </div>
  </ul>
  <%end%>
</nav>
