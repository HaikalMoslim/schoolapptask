<%= simple_form_for @school, local: true, data: { turbo: false } do |form| %>

  <div class="m-10">
  <%= form.label :name %><br>
  <%= form.text_field :name%>
  <% @school.errors.full_messages_for(:name).each do |message| %>
    <div><%= message %></div>
  <% end %>
</div>
<div class="m-10">
  <%= form.label :address %><br>
  <%= form.text_field :address %>
  <% @school.errors.full_messages_for(:address).each do |message| %>
    <div><%= message %></div>
  <% end %>
</div>
<div class="m-10">
  <%= form.label :city %><br>
  <%= form.text_field :city%>
  <% @school.errors.full_messages_for(:city).each do |message| %>
    <div><%= message %></div>
  <% end %>
</div>
<div class="m-10">
  <%= form.label :state %><br>
  <%= form.text_field :state, class: 'form-control' %>
  <% @school.errors.full_messages_for(:state).each do |message| %>
    <div><%= message %></div>
  <% end %>
</div>
<div class="m-10">
  <%= form.label :link %><br>
  <%= form.text_field :link, class: 'form-control' %>
  <% @school.errors.full_messages_for(:link).each do |message| %>
    <div><%= message %></div>
  <% end %>
</div>


<div class="m-10">
  <%= form.submit %>
</div>
<% end %>